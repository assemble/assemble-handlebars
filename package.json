{
  "name": "assemble-handlebars",
  "description": "Handlebars engine plugin for grunt-assemble. This is only used in grunt-assemble and versions of assemble below 0.4.x.",
  "version": "0.4.1",
  "homepage": "https://github.com/assemble/assemble-handlebars",
  "author": "Brian Woodward (https://github.com/doowb)",
  "maintainers": [
    "Brian Woodward (https://github.com/doowb)",
    "Jon Schlinkert (https://github.com/jonschlinkert)"
  ],
  "contributors": [
    "Anders D. Johnson (https://andrz.me)",
    "Brian Woodward <brian.woodward@gmail.com> (https://twitter.com/doowb)",
    "Jon Schlinkert <jon.schlinkert@sellside.com> (http://twitter.com/jonschlinkert)",
    "Laurent Goderre <laurent.goderre@gmail.com> (https://github.com/LaurentGoderre)",
    "Paul Welsh <spacedawwwg@gmail.com> (paulwelsh.info)"
  ],
  "repository": "assemble/assemble-handlebars",
  "bugs": {
    "url": "https://github.com/assemble/assemble-handlebars/issues"
  },
  "license": "MIT",
  "files": [
    "./index.js",
    "index.js"
  ],
  "main": "./index.js",
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "test": "grunt test"
  },
  "dependencies": {
    "handlebars": "^4.0.6",
    "handlebars-helpers": "^0.8.0"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-mocha-test": "^0.13.2",
    "gruntify-eslint": "^3.1.0",
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "assemble",
    "assembleengine",
    "handlebars",
    "helpers",
    "template",
    "templates"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "grunt",
        "grunt-assemble",
        "handlebars-helpers"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
